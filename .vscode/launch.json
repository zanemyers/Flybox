{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Debugger",
      "configurations": ["Local", "Docker"]
    }
  ],
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Local",
      "address": "localhost",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Docker",
      "address": "localhost",
      "port": 9229,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
